{
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "temporary-event-cms.vercel.app" }],
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": ":subdomain.tempevents.vercel.app" }],
      "destination": "/website.html"
    }
  ]
}
